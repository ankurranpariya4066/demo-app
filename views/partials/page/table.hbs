<table id="myTable" class="display">
    <thead>
        <tr>
            <th>Listing ID</th>
            <th>Listing Month</th>
            <th>Listing Date</th>
            <th>Broker</th>
            <th>Revenue</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.listing_month}}</td>
            <td>{{this.listing_full_date}}</td>
            <td>{{this.title}}</td>
            <td>{{this.revenue}}</td>
        </tr>
        {{/each}}
    </tbody>
    <tfoot>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tfoot>
</table>